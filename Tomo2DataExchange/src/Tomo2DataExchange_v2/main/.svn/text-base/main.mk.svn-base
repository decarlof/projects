	
MAIN_SRC = ExampleUsage

MAIN_OBJS = ./main/ExampleUsage.o

MAIN_INSTALL = main_install
				
ExampleUsage: $(ExampleUsage)
	$(CCC) $(CCFLAGS) \
	$(NAPI_INC) $(HDF_INC) $(UTILITY_INC) \
	$(NEXUSLIB_INC) $(HDF_FLAGS) \
	-c ./main/ExampleUsage.cpp \
	-o ./main/ExampleUsage.o 
	@echo ' ' 			
	
main_link: $(main_link)
	$(LD) $(NEXUS_LIBS) $(OTHER_LIBS) \
	$(HDF_LIBS) $(HDF5_LIBS) \
	$(NAPI_OBJS) $(UTILITY_OBJS) \
	./main/ExampleUsage.o -o ./main/ExampleUsage

main_install: $(main_install)
	cp ./main/ExampleUsage ./bin/ExampleUsage
	
main_clean : $(main_clean)
	/bin/rm -f ./main/ExampleUsage
	


